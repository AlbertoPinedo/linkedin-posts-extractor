{
  "manifest_version": 3,
  "name": "LinkedIn Post Extractor",
  "version": "1.0",
  "description": "Extract your LinkedIn posts",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["popup.js"]
    }
  ]
}
